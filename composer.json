{
    "name": "easyengine/site-command",
    "description": "",
    "type": "ee-cli-package",
    "homepage": "https://github.com/easyengine/site-command",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "": "src/",
            "AcmePhp\\Core\\": "AcmePhp/Core",
            "AcmePhp\\Ssl\\": "AcmePhp/Ssl",
            "AcmePhp\\Cli\\": "AcmePhp/Cli"
            
        },
        "files": [ "site-command.php" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bundled": true,
        "commands": [
            "site",
            "site create",
            "site list",
            "site delete"
        ]
    },
    "require": {
        "ext-openssl": "*",
        "webmozart/assert": "^1.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/serializer": "^3.0"
    }
}
